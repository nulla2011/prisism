    "@vitejs/plugin-vue": "^4.6.2",
    "ant-design-vue": "^4.1.1",
    "autoprefixer": "^10.4.14",
    "crypto-js": "^4.2.0",
    "electron": "25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
